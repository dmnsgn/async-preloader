{
  "name": "async-preloader",
  "version": "6.1.1",
  "description": "Assets preloader using ES2017 async/await and fetch.",
  "keywords": [
    "preloader",
    "assets",
    "async",
    "await",
    "typescript",
    "ES2017"
  ],
  "homepage": "https://github.com/dmnsgn/async-preloader",
  "bugs": "https://github.com/dmnsgn/async-preloader/issues",
  "repository": "dmnsgn/async-preloader",
  "funding": [
    {
      "type": "individual",
      "url": "https://paypal.me/dmnsgn"
    },
    {
      "type": "individual",
      "url": "https://commerce.coinbase.com/checkout/56cbdf28-e323-48d8-9c98-7019e72c97f3"
    }
  ],
  "license": "MIT",
  "author": "Damien Seguin (https://github.com/dmnsgn)",
  "type": "module",
  "exports": "./lib/index.js",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "prejest": "npx snowdev install",
    "jest": "NODE_OPTIONS=--experimental-vm-modules npx jest test/ --watch --coverage --no-cache",
    "pretest": "npx snowdev install && npx snowdev build --no-docs --no-lint",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest test/ --runInBand"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "coverage",
    "globalSetup": "jest-environment-puppeteer/setup",
    "globalTeardown": "jest-environment-puppeteer/teardown",
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "preset": "ts-jest/presets/js-with-ts-esm",
    "resolver": "jest-ts-webcompat-resolver",
    "setupFiles": [
      "<rootDir>/test/setupFile.js"
    ],
    "setupFilesAfterEnv": [
      "expect-puppeteer"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|js)x?$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "fontfaceobserver-es": "^3.3.3",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/css-font-loading-module": "^0.0.7",
    "@types/expect-puppeteer": "^4.4.7",
    "@types/jest": "^27.4.1",
    "@types/jest-environment-puppeteer": "^5.0.0",
    "@types/node": "^17.0.23",
    "@types/puppeteer": "^5.4.5",
    "@xmldom/xmldom": "^0.8.1",
    "es-module-shims": "^1.5.2",
    "jest": "^27.5.1",
    "jest-puppeteer": "^6.1.0",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "jsdom": "^19.0.0",
    "mime-types": "^2.1.35",
    "node-fetch": "^3.2.3",
    "puppeteer": "^13.5.2",
    "snowdev": "^1.10.0",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "snowdev": {
    "dependencies": [
      "fontfaceobserver-es",
      "es-module-shims"
    ]
  }
}
